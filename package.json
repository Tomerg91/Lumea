{
  "name": "satya-coaching",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "npm run dev --workspace client & npm run dev --workspace server",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "build": "npm run build --workspace client && npm run build --workspace server",
    "typecheck": "npm run typecheck --workspace client && npm run typecheck --workspace server",
    "check": "cd client && npm run typecheck && cd ../server && npm run typecheck",
    "ci-check": "npm run check",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "cd client && npm run test -- --reporter verbose && cd ../server && npm run test -- --verbose",
    "ci-all": "npm run lint && npm run ci-check && npm run test",
    "db:migrate": "supabase db reset",
    "db:seed": "ts-node scripts/bootstrap.ts",
    "test:rls": "vitest run supabase/tests/rls.spec.ts",
    "prepare": "[ -z \"$CI\" ] && husky || echo 'Skipping husky setup in CI environment'",
    "migrate-data": "tsx scripts/migrate-data.ts",
    "test-supabase": "tsx scripts/migrate-data.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "^7.2.0",
    "@capacitor/core": "7.2.0",
    "@eslint/js": "9.25.1",
    "@supabase/supabase-js": "^2.39.8",
    "@testing-library/jest-dom": "6.6.3",
    "@types/bcryptjs": "2.4.6",
    "@types/connect-pg-simple": "7.0.3",
    "@types/connect-redis": "0.0.23",
    "@types/jest": "29.5.14",
    "@types/libsodium-wrappers": "0.7.14",
    "@types/pg": "8.11.14",
    "@types/redis": "4.0.10",
    "@types/testing-library__react": "10.0.1",
    "@typescript-eslint/parser": "^8.29.0",
    "bcryptjs": "3.0.2",
    "concurrently": "^8.2.2",
    "connect-pg-simple": "10.0.0",
    "dotenv": "^16.3.1",
    "drizzle-zod": "0.7.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "memorystore": "1.6.7",
    "pg": "8.15.6",
    "postgres": "3.4.5",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.1",
    "vite-plugin-compression2": "1.3.3",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@capacitor/android": "^7.2.0",
    "@capacitor/filesystem": "7.0.1",
    "@capacitor/ios": "^7.2.0",
    "@headlessui/react": "2.2.2",
    "@radix-ui/react-icons": "1.3.2",
    "@testing-library/react": "16.3.0",
    "axios": "1.9.0",
    "connect-redis": "8.0.3",
    "date-fns": "2.30.0",
    "express-validator": "^7.2.1",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.5.0",
    "libsodium-wrappers": "0.7.15",
    "rate-limit-redis": "4.2.1",
    "react-i18next": "^14.1.0",
    "redis": "5.0.0",
    "task-master-ai": "^0.12.1"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "overrides": {
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.7",
    "typescript": "5.8.3"
  }
}
